{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0.7",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1.11.99.2",
        "doctrine/doctrine-bundle": "^2.4.2",
        "doctrine/doctrine-migrations-bundle": "^3.1.1",
        "doctrine/orm": "^2.9.3",
        "friendsofsymfony/ckeditor-bundle": "^2.3.0",
        "leapt/core-bundle": "^3.2.1",
        "matomo/device-detector": "^3.13.1",
        "sensio/framework-extra-bundle": "^6.1.5",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.13.3",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/monolog-bundle": "^3.7.0",
        "symfony/security-bundle": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/webpack-encore-bundle": "^1.12.0",
        "symfony/yaml": "5.3.*",
        "twig/extra-bundle": "^3.3.1",
        "twig/twig": "^3.3.2"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.9.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.90",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-doctrine": "^0.12.39",
        "phpstan/phpstan-phpunit": "^0.12.20",
        "phpstan/phpstan-symfony": "^0.12.37",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "5.3.*",
        "symfony/css-selector": "5.3.*",
        "symfony/debug-bundle": "5.3.*",
        "symfony/phpunit-bridge": "5.3.*",
        "symfony/stopwatch": "5.3.*",
        "symfony/var-dumper": "5.3.*",
        "symfony/web-profiler-bundle": "5.3.*",
        "symplify/easy-coding-standard": "^9.3.27"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop --release=custom --custom-build-id=2a9f83788920fcf21eb18cc2d50b2f66": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "ci": [
            "bin/console doctrine:schema:validate --ansi",
            "@phpcpd",
            "@cs:dry",
            "@phpstan",
            "bin/console lint:twig templates --ansi",
            "bin/console lint:yaml config database/fixtures translations --ansi"
        ],
        "cs:dry": "vendor/bin/ecs --ansi",
        "cs:fix": "vendor/bin/ecs --fix --ansi",
        "phpcpd": "phpcpd src tests",
        "phpstan": "vendor/bin/phpstan analyse --ansi"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
