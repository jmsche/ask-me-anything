{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0.7",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1.11.99.5",
        "doctrine/doctrine-bundle": "^2.6.3",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.12.2",
        "friendsofsymfony/ckeditor-bundle": "^2.4.0",
        "google/recaptcha": "^1.2.4",
        "leapt/core-bundle": "^3.3.2",
        "matomo/device-detector": "^3.13.1",
        "sensio/framework-extra-bundle": "^6.2.6",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^2.1.8",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.8.0",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.14.1",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^3.3.8",
        "twig/twig": "^3.3.10"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.10.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-doctrine": "^0.12.44",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpstan/phpstan-symfony": "^0.12.44",
        "phpunit/phpunit": "^9.5.20",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/var-dumper": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "symplify/easy-coding-standard": "^10.2.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop --release=custom --custom-build-id=2a9f83788920fcf21eb18cc2d50b2f66": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "ci": [
            "bin/console doctrine:schema:validate --ansi",
            "@phpcpd",
            "@cs:dry",
            "@phpstan",
            "bin/console lint:twig templates --ansi",
            "bin/console lint:yaml config database/fixtures translations --ansi"
        ],
        "cs:dry": "vendor/bin/ecs --ansi",
        "cs:fix": "vendor/bin/ecs --fix --ansi",
        "phpcpd": "phpcpd src tests",
        "phpstan": "vendor/bin/phpstan analyse --ansi"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
